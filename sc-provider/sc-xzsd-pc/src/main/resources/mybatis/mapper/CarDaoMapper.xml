<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xzsd.pc.car.dao.CarDao">
    <!--查询车辆总数-->
    <select id="queryCarSum" resultType="java.lang.String">
        select
            count(*)
        from
            t_sys_car
        where
            is_deleted = 0
    </select>
</mapper>
